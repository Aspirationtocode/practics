			.tabs-content-data
				+tabs-content-data-mto()
		.tabs-content-element.tabs-content-element--4
			.tabs-content-data
				+tabs-content-data-teach-work()
		.tabs-content-element.tabs-content-element--5
			.tabs-content-data
				+tabs-content-data-stuff()
		.tabs-content-element.tabs-content-element--6
			.tabs-content-data
				+tabs-content-data-documents(documentsData)
